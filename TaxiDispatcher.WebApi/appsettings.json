{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "Enable": true,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Component": "TaxiDispatcher",
      "Application": "TaxiDispatcher.WebApi"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Log/LogFile.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
